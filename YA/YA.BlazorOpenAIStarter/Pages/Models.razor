@* @page "/models/{ModelName}" *@
@page "/models"
@inject NavigationManager Navigation





@if (ModelName == "chatgpt")
{
    <button class="btn btn-danger mb-3" @onclick="@(()=> Navigation.NavigateTo("/models?modelName=dall-e"))"> Switch to ChatGPT Page</button>
    <ChatGpt />
}
else
{
    <button class="btn btn-danger mb-3" @onclick="@(()=> Navigation.NavigateTo("/models?modelName=chatgpt"))"> Switch to ChatGPT Page</button>
    <DallE />

}


@code {
    // [Parameter]
    // public string ModelName { get; set; }

[SupplyParameterFromQuery(Name ="modelName")]
    [Parameter]
    public string ModelName { get; set; }

}
